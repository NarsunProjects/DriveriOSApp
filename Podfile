  pod 'IQKeyboardManager'
  pod 'SAMKeychain'
  pod 'ImagePicker'
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  # Pods for Instagruas

  target 'InstagruasTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'InstagruasUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
